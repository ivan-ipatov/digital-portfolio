{
  "name": "gravity-ui-example-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint:js": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint:styles": "stylelint '{styles,src}/**/*.scss'",
    "lint:prettier": "prettier --check '**/*.md'",
    "lint": "npm run lint:js && npm run lint:styles && npm run lint:prettier"
  },
  "dependencies": {
    "@gravity-ui/icons": "^2.6.0",
    "@gravity-ui/uikit": "^5.17.0",
    "bem-cn-lite": "^4.1.0",
    "next": "^13.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@gravity-ui/browserslist-config": "^4.1.0",
    "@gravity-ui/eslint-config": "^3.1.1",
    "@gravity-ui/prettier-config": "^1.1.0",
    "@gravity-ui/stylelint-config": "^4.0.0",
    "@gravity-ui/tsconfig": "^1.0.0",
    "@svgr/webpack": "^8.1.0",
    "@types/node": "^20.7.0",
    "@types/react": "^18.2.23",
    "@types/react-dom": "^18.2.8",
    "eslint": "^8.50.0",
    "eslint-config-next": "^13.5.3",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "sass": "^1.68.0",
    "sass-loader": "^13.3.2",
    "stylelint": "^15.10.3",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18"
  },
  "browserslist": [
    "extends @gravity-ui/browserslist-config"
  ],
  "overrides": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "lint-staged": {
    "*.{scss}": [
      "stylelint --fix --quiet"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --quiet",
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  }
}
